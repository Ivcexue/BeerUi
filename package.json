    "acorn": "^8.7.1",
    "async-validator": "^4.1.1",
    "autoprefixer": "^10.4.1",
    "cssnano": "^5.0.14",
    "estree-walker": "^3.0.1",
    "marked": "^4.0.16",
    "postcss": "^8.3.9",
    "rollup": "^2.75.6",
    "sourcemap-codec": "^1.4.8",
    "svelte-json-tree": "^1.0.0",
    "yootils": "^0.3.1"
  }
