{
  "name": "@brewer/beerui",
  "version": "1.1.9",
  "description": "A Svelte Ui Components",
  "main": "index.js",
  "scripts": {
    "build": "vite build",
    "build:office": "cross-env NODE_ENV=office vite build",
    "publish": "cd package && npm publish",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "vite dev --port 3333 --host",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "lint": "eslint --ignore-path .gitignore .",
    "package": "svelte-package",
    "preview": "vite preview",
    "install": "pnpm install",
    "preinstall": "npx only-allow pnpm",
    "test": "start-server-and-test storybook http://localhost:6006 cy:run"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@codewithshin/svelte-utterances": "^0.2.2",
    "@sveltejs/adapter-static": "2.0.1",
    "@sveltejs/adapter-vercel": "2.3.0",
    "@sveltejs/kit": "1.10.0",
    "@sveltejs/package": "^2.0.2",
    "@sveltejs/vite-plugin-svelte": "^2.0.3",
    "@tsconfig/svelte": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "babel-loader": "^8.2.5",
    "codemirror": "^5.65.9",
    "cross-env": "^7.0.3",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "highlight.js": "^11.6.0",
    "postcss-load-config": "^4.0.1",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.1",
    "sass": "^1.55.0",
    "svelte-check": "^3.0.4",
    "svelte-loader": "^3.1.7",
    "svelte-preprocess": "^5.0.1",
    "svelte2tsx": "^0.6.2",
    "sveltekit-static-sitemap": "^1.0.2",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "homepage": "https://github.com/beerui/BeerUi",
  "prettier": {
    "singleQuote": true,
    "tabWidth": 4,
    "useTabs": true
  },
  "bugs": {
    "url": "https://github.com/beerui/BeerUi/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/beerui/BeerUi.git"
  },
  "type": "module",
  "license": "MIT",
  "keywords": [
    "svelte",
    "beerui",
    "components",
    "UI",
    "framework",
    "templates"
  ],
  "author": "beerui",
  "dependencies": {
    "acorn": "^8.8.0",
    "async-validator": "^4.2.5",
    "autoprefixer": "^10.4.12",
    "cssnano": "^5.1.13",
    "estree-walker": "^3.0.1",
    "marked": "^4.1.0",
    "postcss": "^8.4.16",
    "rollup": "^2.79.1",
    "sourcemap-codec": "^1.4.8",
    "svelte": "^3.55.1",
    "svelte-json-tree": "^1.0.0",
    "yootils": "^0.3.1"
  },
  "peerDependencies": {
    "vite": "4.0.1"
  }
}
