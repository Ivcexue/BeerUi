{
  "name": "@brewer/beerui",
  "version": "1.0.30-next.46",
  "description": "A Svelte Ui Components",
  "main": "index.js",
  "scripts": {
    "build": "svelte-kit build",
    "build-scss": "cd src/lib/assets/scss/ && npm i && npm run build",
    "publish": "cd package && npm publish",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "svelte-kit dev --port 3333 --host",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "lint": "eslint --ignore-path .gitignore .",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "preinstall": "npx only-allow pnpm",
    "test": "start-server-and-test storybook http://localhost:6006 cy:run"
  },
  "devDependencies": {
    "@babel/core": "7.16.7",
    "@babel/preset-env": "7.16.7",
    "@sveltejs/adapter-vercel": "^1.0.0-next.47",
    "@sveltejs/kit": "^1.0.0-next.311",
    "@tsconfig/svelte": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "5.9.0",
    "@typescript-eslint/parser": "5.9.0",
    "babel-loader": "8.2.3",
    "eslint": "8.6.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-storybook": "^0.5.5",
    "eslint-plugin-svelte3": "3.2.1",
    "highlight.js": "^11.4.0",
    "prettier": "2.5.1",
    "prettier-plugin-svelte": "2.5.1",
    "postcss": "^8.3.9",
    "postcss-load-config": "^3.1.1",
    "sass": "^1.45.2",
    "svelte": "^3.47.0",
    "svelte-check": "2.2.11",
    "svelte-loader": "3.1.2",
    "svelte-preprocess": "^4.10.5",
    "svelte2tsx": "0.4.12",
    "tslib": "2.3.1",
    "typescript": "4.5.4"
  },
  "homepage": "https://github.com/beerui/BeerUi",
  "prettier": {
    "singleQuote": true,
    "tabWidth": 4,
    "useTabs": true
  },
  "bugs": {
    "url": "https://github.com/beerui/BeerUi/issues"
  },
  "repository": {
    "type": "git",
